@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">Hata oluştu.</h2>


@if(Model.Errors.Any())
{
    <div class="alert alert-danger">
        @foreach (var item in Model.Errors)
        {
            <p>@item</p>
        }
    </div>
}